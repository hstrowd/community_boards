<p id="notice"><%= notice %></p>

<div id='page_title'>
  <% @community %>
</div>

<div id='events_list'>
  <% if(!@community.events.empty?) %>
    <ul>
      <% @community.events.each do |event| %>
	<li><%= link_to event, event_path(event) %></li>
      <% end %>
    </ul>
  <% else %>
    This community currently has no events registered for it. <%= link_to "Add one", new_event_path %>.
  <% end %>
